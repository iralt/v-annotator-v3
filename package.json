{
  "name": "v-annotator-v3",
  "version": "0.1.0",
  "description": "Vue 3 annotation library with Vuetify",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "types",
    "README.md"
  ],
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir types",
    "build:demo": "vite build --mode demo --outDir dist-demo",
    "preview": "vite preview",
    "preview:demo": "vite preview --outDir dist-demo",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "5.2.4",
    "typescript": "^5.0.0",
    "vite": "5.4.19",
    "vue": "^3.5.17",
    "vue-tsc": "2.2.12",
    "vuetify": "3.8.12"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuetify",
    "annotation",
    "library"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iralt/v-annotator-v3.git"
  },
  "dependencies": {
    "@flatten-js/interval-tree": "^1.1.3",
    "grapheme-splitter": "^1.0.4",
    "lodash-es": "^4.17.21",
    "uuid": "^11.1.0"
  }
}
